<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="de.sindzinski.lpitrainer">

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="16"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="de.sindzinski.lpitrainer.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_test" android:name="de.sindzinski.lpitrainer.TestActivity" android:parentActivityName="com.example.myfirstapp.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="de.sindzinski.lpitrainer.MainActivity"/>
        </activity>
        <activity android:label="@string/title_activity_settings" android:name="de.sindzinski.lpitrainer.SettingsActivity">
        </activity>
    </application>

</manifest>
